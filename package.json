{
  "name": "element-plus-lego",
  "version": "1.5.8",
  "description": "Element Plus 二次封装组件库，提供更强大和易用的企业级组件",
  "main": "./dist/element-plus-lego.cjs.js",
  "module": "./dist/element-plus-lego.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/element-plus-lego.es.js",
      "require": "./dist/element-plus-lego.cjs.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm --filter @element-plus-lego/play dev",
    "build": "pnpm --filter @element-plus-lego/play build",
    "build:lib": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "pnpm --filter @element-plus-lego/play preview",
    "docs:dev": "pnpm --filter @element-plus-lego/docs dev",
    "docs:build": "pnpm --filter @element-plus-lego/docs build",
    "docs:preview": "pnpm --filter @element-plus-lego/docs preview",
    "prepublishOnly": "pnpm run build:lib"
  },
  "keywords": [
    "vue",
    "vue3",
    "element-plus",
    "components",
    "ui",
    "enterprise",
    "pro",
    "table",
    "form",
    "search"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/element-plus-lego"
  },
  "bugs": {
    "url": "https://github.com/yourusername/element-plus-lego/issues"
  },
  "homepage": "https://github.com/yourusername/element-plus-lego#readme",
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "element-plus": "^2.0.0"
  },
  "dependencies": {
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "element-plus": "^2.11.2",
    "lodash-es": "^4.17.21",
    "@element-plus/icons-vue": "^2.3.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "postcss": "^8.5.6",
    "sass": "^1.92.1",
    "typescript": "~5.8.3",
    "unocss": "^66.5.1",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5",
    "code-inspector-plugin": "^1.2.10"
  }
}
